{
  "name": "mapbox-test-automation",
  "version": "1.0.0",
  "description": "Mapbox Test Automation Framework",
  "scripts": {
    "test": "mocha 'test/**/*.spec.js' --require @babel/register --timeout 30000",
    "test:report": "mocha 'test/**/*.spec.js' --require @babel/register --timeout 30000 --reporter mochawesome",
    "open:report": "npx serve ./mochawesome-report"
  },
  "dependencies": {
    "selenium-webdriver": "^4.15.0",
    "chromedriver": "^119.0.1",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "@babel/core": "^7.23.3",
    "@babel/register": "^7.22.15",
    "@babel/preset-env": "^7.23.3",
    "dotenv": "^16.3.1",
    "serve": "^14.2.1"
  }
}